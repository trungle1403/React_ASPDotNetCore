    <style>
        body {
            font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
            font-size: 12px;
            font-size-adjust: inherit;
            font-stretch: normal;
            font-variant: normal;
            font-style: normal;
        }

        .bootbox.modal.bb-alternate-modal.in {
            top: 10% !important;
        }

        button.bootbox-close-button.close {
            margin-top: -6px !important;
        }

        .close {
            float: right;
            font-size: 21px;
            color: #f00;
            opacity: 1;
        }

        .ntsloadding {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 9999;
            opacity: 0.5;
            text-align: center;
            background: #CCC;
            filter: alpha(opacity=80);
        }

        .message-loading-overlay > .ace-icon {
            position: absolute;
            top: 40%;
            left: 49%;
            right: 0;
            text-align: center;
        }
        h5.smaller {
            margin-top: 0px;
        }
    </style>
